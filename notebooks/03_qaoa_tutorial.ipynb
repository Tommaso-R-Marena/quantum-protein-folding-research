   "source": [
    "## 5. QAOA vs VQE Head-to-Head"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "sequence = \"HPHPPHHPHH\"\n",
    "\n",
    "# QAOA\n",
    "print(\"Running QAOA...\")\n",
    "qaoa_model = QAOAFoldingModel(sequence=sequence, p_layers=3, lattice_dim=2)\n",
    "qaoa_start = time.time()\n",
    "qaoa_result = qaoa_model.run(maxiter=100)\n",
    "qaoa_time = time.time() - qaoa_start\n",
    "\n",
    "# VQE\n",
    "print(\"Running VQE...\")\n",
    "vqe_model = VQEFoldingModel(sequence=sequence, ansatz_depth=3, lattice_dim=2)\n",
    "vqe_start = time.time()\n",
    "vqe_result = vqe_model.run(maxiter=100)\n",
    "vqe_time = time.time() - vqe_start\n",
    "\n",
    "# Comparison\n",
    "print(\"\\n" + \"=\" * 60)\n",
    "print(\"QAOA vs VQE Comparison\")\n",
    "print(\"=\" * 60)\n",
    "print(f\"{'Metric':<30} {'QAOA':<15} {'VQE':<15}\")\n",
    "print(\"-\" * 60)\n",
    "print(f\"{'Optimal Energy':<30} {qaoa_result.optimal_value:<15.4f} {vqe_result.optimal_value:<15.4f}\")\n",
    "print(f\"{'Time (s)':<30} {qaoa_time:<15.2f} {vqe_time:<15.2f}\")\n",
    "print(f\"{'Iterations':<30} {qaoa_result.n_iterations:<15} {vqe_result.n_iterations:<15}\")\n",
    "print(f\"{'Parameters':<30} {len(qaoa_result.optimal_params):<15} {len(vqe_result.optimal_params):<15}\")\n",
    "print(\"=\" * 60)\n",
    "\n",
    "# Winner\n",
    "if qaoa_result.optimal_value < vqe_result.optimal_value:\n",
    "    print(\"\\nðŸ† QAOA found a better solution!\")\n",
    "elif vqe_result.optimal_value < qaoa_result.optimal_value:\n",
    "    print(\"\\nðŸ† VQE found a better solution!\")\n",
    "else:\n",
    "    print(\"\\nðŸ¤ Tie! Both algorithms found the same energy.\")\n",
    "\n",
    "# Plot convergence comparison\n",
    "fig, ax = plt.subplots(figsize=(10, 6))\n",
    "\n",
    "ax.plot(qaoa_result.convergence_history, label='QAOA', linewidth=2)\n",
    "ax.plot(vqe_result.convergence_history, label='VQE', linewidth=2)\n",
    "\n",
    "ax.set_xlabel('Iteration', fontsize=12)\n",
    "ax.set_ylabel('Energy', fontsize=12)\n",
    "ax.set_title('QAOA vs VQE Convergence', fontweight='bold', fontsize=14)\n",
    "ax.legend(fontsize=12)\n",
    "ax.grid(True, alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.savefig('../results/qaoa_vs_vqe.png', dpi=300, bbox_inches='tight')\n",
    "plt.show()"
   ]
  },
